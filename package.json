{
  "name": "@jeswr/comunica-perf",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test": "jest",
    "lint": "eslint lib/* __tests__/* --ext .ts",
    "lint:fix": "eslint lib/* __tests__/* --ext .ts --fix",
    "build:tsc": "tsc",
    "build:webpack": "webpack --env production",
    "build": "npm run build:tsc && npm run build:webpack",
    "prepare": "tsc",
    "perf": "ts-node lib/index.ts",
    "semantic-release": "semantic-release",
    "serve": "http-serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeswr/comunica-perf"
  },
  "keywords": [],
  "author": "Jesse Wright <https://github.com/jeswr/>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/comunica-perf/issues"
  },
  "homepage": "https://github.com/jeswr/comunica-perf/README.md",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "http-serve": "^1.0.1",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "querystring-es3": "^0.2.1",
    "semantic-release": "^24.0.0",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "url": "^0.11.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@comunica/actor-http-fetch": "^3.1.0",
    "@comunica/actor-http-native": "^3.2.0",
    "@comunica/bus-http": "^3.2.0",
    "@comunica/core": "^3.2.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "webpack-node-externals": "^3.0.0"
  }
}
